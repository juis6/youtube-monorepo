{
  "name": "youtube-monorepo",
  "packageManager": "yarn@4.11.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "start:web": "yarn workspace @monorepo/web-app start",
    "start:api": "yarn workspace @monorepo/api-server tsc src/index.ts",
    "dev": "concurrently \"yarn start:web\" \"yarn start:api\""
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "typescript": "^5.9.3"
  }
}
